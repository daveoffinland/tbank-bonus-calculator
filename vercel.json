{
  "version": 2,
  "functions": {
    "index.js": {
      "runtime": "@vercel/node"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ],
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ]
}
